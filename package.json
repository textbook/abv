{
  "name": "abv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.20",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "cypress": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "e2e": "concurrently -k -s first -n \"app,e2e\" \"npm:e2e:start\" \"npm:e2e:test\"",
    "e2e:start": "cross-env BROWSER=none PORT=3001 npm start",
    "pree2e:test": "wait-on -l -t 30000 http-get://localhost:3001",
    "e2e:test": "cross-env CYPRESS_BASE_URL=http://localhost:3001 cypress run",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "extends": [
          "plugin:cypress/recommended"
        ],
        "files": [
          "cypress/**/*.js"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
